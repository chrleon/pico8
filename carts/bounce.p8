pico-8 cartridge // http://www.pico-8.com
version 30
__lua__
x = 10
y = 10
acc = 5
function _draw ()
	cls(1)
	circfill(x,y,4)
	x = x + acc
	collide()
end

function collide()
	if (x > 127)
		then
			acc = acc * -1
			y = y + 1
	end
	if (x <= 0)
		then
			acc = acc * -1
			y = y + 1
	end
	if ( y > 128)
		then
			y = 0
		end
end
